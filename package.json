    "net": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.2",
    "react-wallet-selector": "^2.6.7",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "snarkyjs": "^0.9.6",
    "stream": "0.0.2",
    "stream-browserify": "^3.0.0",
    "tls": "0.0.1",
    "web-vitals": "^2.1.4"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-flow"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
