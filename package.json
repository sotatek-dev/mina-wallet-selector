    "@metamask/detect-provider": "^2.0.0",
    "@reduxjs/toolkit": "^1.9.3",
    "antd": "^5.3.2",
    "axios": "^1.3.4",
    "bootstrap": "^5.2.3",
    "bs58check": "^3.0.1",
    "buffer": "^6.0.3",
    "ethers": "^5.6.8",
